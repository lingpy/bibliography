@article{Gell-Mann2010,
    Publisher = {National Academy of Sciences},
    Doi = {http://dx.doi.org/10.1073/pnas.1113716108},
    Author = {Gell-Mann, Murray and Ruhlen, Merritt},
    Url = {http://www.pnas.org/content/108/42/17290},
    Abstract = {Recent work in comparative linguistics suggests that all, or almost all, attested human languages may derive from a single earlier language. If that is so, then this language\textemdashlike nearly all extant languages\textemdashmost likely had a basic ordering of the subject (S), verb (V), and object (O) in a declarative sentence of the type \textquotedblleftthe man (S) killed (V) the bear (O).\textquotedblright When one compares the distribution of the existing structural types with the putative phylogenetic tree of human languages, four conclusions may be drawn. (i) The word order in the ancestral language was SOV. (ii) Except for cases of diffusion, the direction of syntactic change, when it occurs, has been for the most part SOV &gt; SVO and, beyond that, SVO &gt; VSO/VOS with a subsequent reversion to SVO occurring occasionally. Reversion to SOV occurs only through diffusion. (iii) Diffusion, although important, is not the dominant process in the evolution of word order. (iv) The two extremely rare word orders (OVS and OSV) derive directly from SOV.},
    Title = {The origin and evolution of word order},
    Number = {42},
    Volume = {108},
    Year = {2011},
    Pages = {17290-17295},
    Journal = {Proceedings of the National Academy of Sciences}
}

