@article{Calero2002,
    Doi = {http://dx.doi.org/10.1093/geronb/57.2.P187},
    Author = {Calero, M. Dolores and Arnedo, M. Luisa and Navarro, Elena and Ruiz-Pedrosa, Mónica and Carnero, Cristobal},
    Url = {http://psychsocgerontology.oxfordjournals.org/content/57/2/P187.abstract},
